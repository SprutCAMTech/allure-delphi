{
	"project_path": "../AllureDelphi.dproj",
	"type": "Delphi",
	"sources": [
		"./*.json",
		"$native_project:sources$"
	],

	"title": "SprutCAMTech $native_project:name$.dll",
	"authors": ["SprutCAMTech"],
	"owners":  ["SprutCAMTech"],
	"language": "en-US",
	"version": "1.0", 

	"dependencies": [ 
		{ 
			"NativeDependencies": { 
				"include": "build"
			}
		} 
	],

	"packages": [
		{
			"class": "Nuget",
			"id": "SprutCAMTech.$native_project:name$.x64",
			"group": "x64",
			"variant_files": {
				"Release_x64": [ {
					"files": {
						"$project:output_dll$/$native_project:name$.stbuildresult.json": "build",
						"$project:output_dll$/$native_project:name$.dll": "build"
					}
				} ]
			}
		},
		{
			"class": "Nuget",
			"id": "SprutCAMTech.$native_project:name$.x32",
			"group": "x32",
			"variant_files": {
				"Release_x32": [ {
					"files": {
						"$project:output_dll$/$native_project:name$.stbuildresult.json": "build",
						"$project:output_dll$/$native_project:name$.dll": "build"
					}
				} ]
			}
		}
	]

}
